algoritmo semNome;
// Síntese
//  Objetivo: Digitar 30 caracteres e representar ao contrário.
//  Entrada : Trinta caracteres.
//  Saída   : Trinta caracteres ao contrário.


principal
	
	inteiro indice;
	caracter letra[30];

	para(indice de 0 ate 29 passo 1)faca

		escreval("Digite a ", indice +1,"ª letra:");
		leiaCaracter(letra[indice]);

	fimPara

	para(indice de 29 ate 0 passo -1)faca
		escreval(" ",letra[indice]);
	fimPara
fimPrincipal
