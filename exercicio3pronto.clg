algoritmo semNome;
// Síntese
//  Objetivo:  Somar os numeros de um vetor de 100 posições.
//  Entrada : Numeros.
//  Saída   : Soma dos numeros.


principal

	real dados[100],soma;
	inteiro indice;

	para(indice de 0 ate 99 passo 1)faca
		escreval("Digite o ", indice+1, "º numero: ");
		leia(dados[indice]);
	fimPara
	
	soma=0;
	para(indice de 0 ate 99 passo 1)faca
		soma=soma+dados[indice];
	fimPara
	limpaTela();
	escreval("O resultado da soma é: ", soma);

fimPrincipal
